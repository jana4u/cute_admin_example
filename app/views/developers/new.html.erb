<div class="admin-container-form">
<h1><%= translate(:new, :default => "New {{model}}", :model => Developer.human_name, :scope => [:railties, :scaffold]) %></h1>

<div class="admin-control">
  <div class="back"><%= link_to translate(:back, :default => 'Back', :scope => [:railties, :scaffold]), developers_path %></div>
</div>

<% cute_admin_form_for(@developer) do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :locals => {:f => f} %>
  <div class="admin-submit">
    <%= f.submit translate(:create, :default => "Create", :scope => [:railties, :scaffold]) %>
  </div>
<% end %>
</div>